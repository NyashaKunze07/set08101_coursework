body {
	background-color: white;
}

h3{
	font-family: courier new;
	colour : black;
	text-align: center;
	font-size: 30px;
}
p{
	font-family: verdana;
	font-size: 20px;
	text-align: center;
}
h1{
	text-align:center;
	font-size: 40px
}
ol,ul {
	list-style: none;
}
#mainMenu{
	margin: 10px;
	width: 900px;
	font-family: sans-serif;
}
#mainMenu li{
	display: block;
	width: 120px;
	float:left;
	margin-left:2px;
	
}
.center{
	display: block;
	margin-left: auto;
	margin-right: auto;
	width: 60%;
}
ul.breadcrumb {
  padding: 10px 16px;
  list-style: none;
  background-color: #eee;
}
ul.breadcrumb li {
  display: inline;
  font-size: 18px;
}
ul.breadcrumb li+li:before {
  padding: 8px;
  color: black;
  content: "/\00a0";
}
ul.breadcrumb li a {
  color: #0275d8;
  text-decoration: none;
}
ul.breadcrumb li a:hover {
  color: #01447e;
  text-decoration: underline;
}
